  "controls": {
    "inputSource": "keyboard",
    "external": {
      "mode": "digital",
      "staleTimeoutMs": 160,
      "absolute": {
        "followLerp": 0.5,
        "maxUnitsPerSecond": 8,
        "maxTargetStep": 0.75
      },
      "websocket": {
        "enabled": true,
        "url": "ws://127.0.0.1:8080",
        "reconnectMs": 1000
